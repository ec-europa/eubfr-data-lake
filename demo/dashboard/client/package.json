{
  "name": "@eubfr/demo-dashboard-client",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run prepare && react-scripts start",
    "build": "npm run prepare && react-scripts build && mv build client/dist",
    "deploy": "sls client deploy",
    "prepare":
      "mkdir -p ./src/meta && ln -f ../server/.serverless/stack-output.json ./src/meta/server.json && ln -f ../../../services/value-store/elasticsearch/.serverless/stack-output.json ./src/meta/projects.json",
    "release": "npm run build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "elasticsearch": "14.0.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-s3-uploader": "4.5.1",
    "react-scripts": "1.0.17"
  },
  "devDependencies": {
    "serverless": "1.24.1",
    "serverless-finch": "1.3.0"
  },
  "private": true
}

{
  "name": "@eubfr/demo-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-scripts": "1.0.14"
  },
  "scripts": {
    "start": "npm run copy && react-scripts start",
    "build": "npm run copy && react-scripts build && mv build client/dist",
    "copy":
      "mkdir -p ./src/meta && ln -f ../../services/value-store/projects/stack-output.json ./src/meta/projects.json",
    "deploy": "sls client deploy",
    "release": "npm run build && npm run deploy",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "serverless": "1.23.0",
    "serverless-finch": "1.1.1"
  }
}

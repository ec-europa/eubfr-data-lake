{
  "name": "@eubfr/demo-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "15.6.0",
    "elasticsearch": "14.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "start": "npm run prepare && react-scripts start",
    "build": "npm run prepare && react-scripts build && mv build client/dist",
    "deploy": "sls client deploy",
    "prepare": "serverless deploy && serverless export-env",
    "release": "npm run build && npm run deploy",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "serverless": "1.24.1",
    "serverless-export-env": "arabold/serverless-export-env",
    "serverless-finch": "1.3.0"
  }
}

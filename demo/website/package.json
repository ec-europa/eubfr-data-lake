{
  "name": "@eubfr/demo-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "15.6.1",
    "elasticsearch": "14.2.2",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "npm run prepare && react-scripts start",
    "build": "npm run prepare && react-scripts build && mv build client/dist",
    "deploy": "sls client deploy",
    "prepare": "serverless deploy && serverless export-env",
    "release": "npm run build && npm run deploy",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "serverless": "1.26.1",
    "serverless-export-env": "1.1.3",
    "serverless-finch": "1.4.0"
  }
}
